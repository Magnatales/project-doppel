config_version=5

[application]

config/name="project-doppel"
run/main_scene="res://scenes/Game.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
boot_splash/show_image=false
config/icon="res://textures/godot_103035.svg"
config/windows_native_icon="res://textures/godo4.ico"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="project-doppel"
project/default_link_connection_radius=4.0
project/default_edge_connection_margin=10.0
project/default_cell_size=1.0
project/use_edge_connections=true

